<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :Photo %><br />
    <%= f.file_field :photo %>

  </div>
  <div class="field">
    <%= label_tag 'multiple select' %><br/>
    <%= select_tag "list[]", options_for_select(["Free", "Basic", "Advanced", "Super Platinum"])  , :multiple=>true %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
